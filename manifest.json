{
  "manifest_version": 2,

  "name": "Puppy Overload",
  "description": "This extension returns an overload of labrador puppies.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "/icons/icon.png",
    "default_title": "Puppy Overload"
  },

  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },

  "background": {
    "scripts": ["background.js", "overloadBrowser.js"],
    "persistent": false
  }
}